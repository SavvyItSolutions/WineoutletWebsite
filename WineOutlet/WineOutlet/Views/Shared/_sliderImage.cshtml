
@{
    ViewBag.Title = "_sliderImage";
}


<div class="container">
    <div class="slider-area">

        <!-- slider start -->
        <div class="slider">
            <div id="topSlider" class="nivoSlider nevo-slider">
                <img src="images/1.jpg" alt="main slider" title="#htmlcaption1" />
                <img src="images/2.jpg" alt="main slider" title="#htmlcaption2" />
            </div>
            <div id="htmlcaption1" class="nivo-html-caption slider-caption">
                <div class="slider-progress"></div>
            </div>
            <div id="htmlcaption2" class="nivo-html-caption slider-caption">
                <div class="slider-progress"></div>
            </div>
        </div>
    </div>
    <!-- slider area end -->
</div>
